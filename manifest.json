{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.8.0",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon-48.png"
  },
  "permissions": [
    "tabs",
    "favicon",
    "storage",
    "activeTab",
    "notifications",
    "alarms"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "_favicon/*",
        "manifest.json",
        "about.html",
        "settings.html",
        "about.js",
        "settings.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": [
        "*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      }
    },
    "switch-to-previous-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "__MSG_prevTabDesc__"
    }
  }
}
